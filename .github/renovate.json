{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "azure-pipelines": {
    "enabled": true
  }
  "packageRules": [
    {
      "matchPackagePatterns": [
        "azure-pipelines-task-lib",
        "@types/node",
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ]
}
